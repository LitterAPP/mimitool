.dream-container{ 
    display: flex;    
    font-size: 28rpx;
}
.dream-container form{
  width: 100%;
}
.dream-input-wrap{
     display: flex;
}
.dream-keyword-input{
    width: 100%;
    color: #666;
    padding: 10rpx 10rpx;
    border-bottom: 1rpx solid #dbdbdb;
    margin-left: 10rpx;
    margin-right: 10rpx;
}
.open-dream-btn{
     font-size: 30rpx;
     margin: 15rpx 10rpx;  
     background-color: #13227a;
     color: #fff;
     height: 30px;
}
.dream-result-container{
    scroll-behavior:auto;
}
.dreamResult{
    display: flex;
    flex-direction:column;
    padding: 10rpx 10rpx;
}
.dereamTitle{    
     font-size: 30rpx;    
}
.dereamTitle text{
    font-size: 35rpx; 
    color: #666;     
    border-radius: 25rpx;
    padding: 5rpx 5rpx;
    background-color: papayawhip;
}
.dreamDes{
     font-size: 28rpx;
     color: #000;
     padding: 15rpx 15rpx;
}
