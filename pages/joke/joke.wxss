.joke-container{
    font-size: 28rpx;
}
.hidden{
    display:none;
}
.nav-tab{
    display:flex;     
    flex-direction: row;
    width: 100%;
    height: 100rpx;
    border-bottom: 1rpx solid  beige;
    border-top: 1rpx solid  beige;
}
.tab-default:last-child{
     border-right:0px solid gainsboro;
}
.tab-default{
    line-height:100rpx;
    text-align:center;
    flex: 1;
    border-right:1px solid gainsboro;
    color:#2c2c2c;
    font-weight:bold;
    font-size:28rpx;
    background-image:url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 48rpx 48rpx;
    background-position-y: center;
    background-position-x:100rpx;
}


.tab-selected{
    line-height:100rpx;
    text-align:center;
    flex: 1;
    border-right:1px solid gainsboro;
    color:#1296db;
    font-weight:bold;
    font-size:28rpx;
    background-image:url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 48rpx 48rpx;
    background-position-y: center;
    background-position-x:100rpx;
}
.joke-txt-container{
    height: 200px;
}
.joke-txt-item{
    display: flex;
    flex: 1;
    flex-direction: row;
    padding: 20rpx 20rpx;    
}

.joke-txt-item:nth-child(2n+0){
    border-top: 1rpx solid  gainsboro; 
}


.joke-txt-item text{

    font-size: 28rpx;
    color:  #2c2c2c;
}

.joke-pic-item{
    display: flex;
    flex: 1;
    flex-direction: column;   
    align-items: center;  
}
.joke-pic-item:nth-child(2n+0){
    border-top: 1rpx solid  gainsboro; 
}
.joke-pic-item text{
    padding: 10rpx;
    background-color: lemonchiffon;
    border-radius: 10rpx;
    font-size: 35rpx;
    color:  #2c2c2c;
}